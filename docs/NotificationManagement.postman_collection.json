						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"function addTimeAndFormat(date, secondsToAdd) {",
									"    ",
									"    date.setSeconds(date.getSeconds() + secondsToAdd);",
									"",
									"    let pad = (num, size) => num.toString().padStart(size, '0');",
									"",
									"    let offsetMinutes = date.getTimezoneOffset();",
									"    let offsetSign = offsetMinutes <= 0 ? \"+\" : \"-\";",
									"    let offsetHours = pad(Math.floor(Math.abs(offsetMinutes) / 60), 2);",
									"    let offsetMins = pad(Math.abs(offsetMinutes) % 60, 2);",
									"",
									"    return (",
									"        date.getFullYear() +",
									"        \"-\" + pad(date.getMonth() + 1, 2) +",
									"        \"-\" + pad(date.getDate(), 2) +",
									"        \"T\" + pad(date.getHours(), 2) +",
									"        \":\" + pad(date.getMinutes(), 2) +",
									"        \":\" + pad(date.getSeconds(), 2) +",
									"        \".\" + pad(date.getMilliseconds(), 3) + \"000\" + offsetSign + offsetHours + \":\" + offsetMins",
									"    );",
									"}",
									"",
									"let original = new Date();",
									"pm.collectionVariables.set(\"triggered_time\", addTimeAndFormat(original, 10));",
									"pm.collectionVariables.set(\"upto\", addTimeAndFormat(original, 200));",
									"",
									"",
									"",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
